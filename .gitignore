# This file contains .gitignore rules that are often used with Drupal projects.
# Because .gitignore is specific to your site and its deployment processes, you
# may need to uncomment, add, or remove rules.
#
# To ignore all paths that composer manages when using this project template,
# remove all double-hashed () lines.
webapi/web/sites/default/default.services.yml
webapi/web/sites/default/default.settings.php
webapi/web/sites/development.services.yml
webapi/web/sites/example.settings.local.php

# Place local settings in settings.local.php
webapi/web/sites/*/settings.php
webapi/web/sites/*/settings.local.php
webapi/web/sites/*/services*.yml
!webapi/web/sites/*/services.yml

# Ignore paths that contain user-generated content.
webapi/web/sites/*/files
webapi/web/sites/*/private

# Ignore SimpleTest multi-site environment.
webapi/web/sites/simpletest

# Ignore directories generated by Composer
#
# See the "installer-paths" section in the top-level composer.json
# file.
# ------------------------------------------------------------------
webapi/drush/Commands/contrib/
webapi/web/core/
webapi/web/modules/contrib/
webapi/web/modules/composer/
webapi/web/themes/contrib/
webapi/web/themes/composer/
webapi/web/profiles/contrib/
webapi/web/profiles/composer/
webapi/web/libraries/

# Generally you should only ignore the root vendor directory. It's important
# that core/assets/vendor and any other vendor directories within contrib or
# custom module, theme, etc., are not ignored unless you purposely do so.
vendor/

# Ignore scaffold files
#
# Note that the scaffold plugin may be used to automatically manage
# a site's .gitignore files. If the `vendor` directory is ignored,
# then one or more .gitignore files will be written to also ignore
# any file placed by scaffolding. To avoid the creation of
# additional .gitignore files, add all of the scaffold file
# locations to the top-level .gitignore file, as shown below.
# ------------------------------------------------------------------
webapi/web/.csslintrc
webapi/web/.editorconfig
webapi/web/.eslintignore
webapi/web/.eslintrc.json
webapi/web/.gitattributes
webapi/web/.gitignore
webapi/web/.ht.router.php
webapi/web/.htaccess
webapi/web/INSTALL.txt
webapi/web/README.txt
webapi/web/README.md
webapi/web/autoload.php
webapi/web/example.gitignore
webapi/web/index.php
webapi/web/robots.txt
webapi/web/update.php
webapi/web/web.config
webapi/web/modules/README.txt
webapi/web/profiles/README.txt
webapi/web/sites/README.txt
webapi/web/sites/default/default.services.yml
webapi/web/sites/default/default.settings.php
webapi/web/sites/development.services.yml
webapi/web/sites/example.settings.local.php
webapi/web/sites/example.sites.php
webapi/web/themes/README.txt
.drush-lock-update

# Other common rules
# ------------------
# Ignore files generated by PhpStorm
.idea/
.vscode

# Ignore .env files as they are personal
.env
deploy
webapi/web/deploy
*.sql
*.psd
*.tar
*.gz
*.zip
*.tgz
.ds_store
*.DS_Store
*node_modules
node_modules

# Ignote configuration backup

config/sync/backup_migrate.*
download/
.htpasswd
docker/mariadb/*
mariadb-init/